                  });

                  $('#query').on('focus',function () {
                     if(c24.tools.responsiveSize() === 'xs') {
                        c24.tools.scrollTo('#query',0);
                     }
                  });

                  
               });

               var setQueryPlaceholder = function () {

                  if($('select[name=watchTypes]').val() != '') {
                     $('#query').attr('placeholder', searchTitles.noWatch);
                  }
                  else{
                     switch (c24.tools.responsiveSize()) {
                        case 'lg':
                           $('#query').attr('placeholder', searchTitles.md);
                           break;
                        case 'md':
                           $('#query').attr('placeholder', searchTitles.sm);
                           break;
                        case 'sm':
                        case 'xs':
                           $('#query').attr('placeholder', searchTitles.xs);
                           break;
                        default:
                           break;
                     }
                  }
               };
            </script>
         </div>
      </div>
   </div>
</div>

