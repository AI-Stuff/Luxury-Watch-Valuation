<script type="text/javascript">
      $(window).load(function() {
         c24.flexslider('jq-detail-image', 'thumbnails', function() {
            $('#jq-detail-image .flex-control-thumbs li').each(function(index, listItem) {
               

               listItem = $(listItem);
               var img = listItem.find('img').css('visibility', 'hidden');
               listItem.css('background-image', 'url(' + img.attr('src') + ')');

               // needed if popover for security image is bound to $(listItem)
               $(listItem).click(function() {
                  $('#jq-detail-image .flexslider').flexslider(index);
               });
            });

            

            var zoomButton = $('<span class="flex-zoom" onclick="tce(\'search.detail.lostofferpage\',\'zoom-image\');"><i class="i-search icon-xlg"></i></span>');
            $("#jq-detail-image .flexslider .flex-viewport").append(zoomButton);
            
            //noinspection JSDuplicatedDeclaration
            options = {
               head:   '<div class=\"media\"><div class=\"media-body\"><span class=\"h2 m-t-3 m-l-4 hidden-xs\">Breitling Diamond Breitling Chronometre Chronograph A13352 Automatic Black Dial Box&amp;papers<\/span><\/div><div class=\"media-right\"><span class=\"price-md text-right text-nowrap m-t-3 m-r-1 hidden-xs\">[SOLD]<br \/><\/span><\/div><\/div><\/div>',
               images:  [
                  
                  {
                     
                     url:     'http://cdn.chrono24.com/images/uhren/images_23/s7/5709723gross.jpg?v=1',
                     width:   '480',
                     height:  '480',
                     thumb:   'http://cdn.chrono24.com/images/uhren/images_23/s7/5709723_s140.jpg?v=1'
                     
                  }
               ],
               start: function(mediaSlider) {
                  var slide = $('#jq-detail-image .flex-control-nav li').index($('#jq-detail-image .flex-control-nav li.active'));
                  if (slide == -1) slide = 0;

                  var $detailMediaSlider = $('#jq-media-slider .flexslider');
                  var animationSpeed = $detailMediaSlider.data('flexslider').vars.animationSpeed;
                  $detailMediaSlider.data('flexslider').vars.animationSpeed = 0;
                  $detailMediaSlider.flexslider(slide, false, false, true, false);
                  $detailMediaSlider.data('flexslider').vars.animationSpeed = animationSpeed;
               }
            };

            zoomButton.mediaSlider(options);
         }, true);
      });
   </script>
