<script type="text/javascript">
   window.onerror = function(message, file, line, column, error) {
      
      if ((message == 'Script error.' || message == 'Script error') && (error == null || error == '' || error == message)) return;

      setTimeout(function () {
         if(file == '' || file == ' ' || typeof file == 'undefined' ) {
            file = window.location.href + '->' + window.errorMessageFile;
         }

         
         try {
            if(
               message.indexOf("evaluating 'v.src'") != -1 ||
               message.indexOf("evaluating 'elt.parentNode'") != -1 ||
               message.indexOf("__show__deepen") != -1 ||
               message.indexOf("docs-homescreen-gb-container") != -1 ||
               file.indexOf('cdn2.') != -1 ||
               file.indexOf('cdn.') != -1
            ) {
               return;
            }
         } catch (e) {}

         var sFormattedMessage = '[' + file + ' (' + line + ')] ' + message + ', ' + JSON.stringify(error);
         
         if(sFormattedMessage.search('chrono24') != -1) {
            trackEvent('Exception', 'Javascript', sFormattedMessage, true);
         }
      }, 500);
   };
</script>

<script type="text/javascript">
   function checkForNonLatinChars () {
      var element = document.querySelectorAll('div, span, strong, p, li, th, td, h1, h2, h3, h4');

      if (typeof element == 'undefined') {
         return;
      }

      for(var i = 0; i < element.length; i++) {
         for (var i2 = 0; i2 < element[i].childNodes.length; i2++) {
            if (typeof(element[i].childNodes[i2]) == 'object' && element[i].childNodes[i2].nodeType == 3) {
               var content = element[i].childNodes[i2].innerText || element[i].childNodes[i2].textContent;

               if (content.trim() == '') continue;

               if (containsNonLatinGreekGyrillicCodepoints(content)) {
                  element[i].className += ' font-all-content';
                  break;
               }
            }
         }
      }
   }

   function containsNonLatinGreekGyrillicCodepoints(s) {
      

      return /[^\u0000-\u007F\u0080-\u00FF\u0100-\u017F\u0180-\u024F\u0370-\u03FF\u0400-\u04FF\u0500-\u052F\u1E00-\u1EFF\u1F00-\u1FFF\u2000-\u206F\u2070-\u209F\u20A0-\u20CF\u20D0-\u20FF\u2150-\u218F\u2C60-\u2C7F\uA720-\uA7FF\u0E3F\u2200-\u22FF]/.test(s);
   }

   checkForNonLatinChars();
</script><script type="text/javascript">
   
         var gridSizes = {
            'block': {
               'xs':2,
               'sm':3,
               'md':4,
               'lg':5
            },
            'list': {
               'xs':1,
               'sm':1,
               'md':1,
               'lg':1
            },
            'splitList': {
               'xs':1,
               'sm':1,
               'md':2,
               'lg':2
            }
         };
      
   function searchColumnsCookie() {
        
      var responsiveSize = c24.tools.responsiveSize();
        
      var searchColumns = 'block='+gridSizes.block[responsiveSize] + '&' + 'list=' + gridSizes.list[responsiveSize] + '&' + 'split-list=' + gridSizes.splitList[responsiveSize];
        
      $.cookie("searchColumns", searchColumns, { expires: 180, path: '/' });
   }

   $(window).load(function () {
         $(window).on('resize', function() {
               searchColumnsCookie();
            });
         
         searchColumnsCookie();
      });
</script></body>
